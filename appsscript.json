//this code is to get the correct permission scopes for google


{
  "timeZone": "America/New_York",
  "dependencies": {
    "enabledAdvancedServices": [{
      "userSymbol": "Gmail",
      "serviceId": "gmail",
      "version": "v1"
    }]
  },
  "exceptionLogging": "STACKDRIVER",
  "oauthScopes": ["https://www.googleapis.com/auth/script.send_mail", "https://mail.google.com/", "https://www.googleapis.com/auth/script.container.ui", "https://www.googleapis.com/auth/documents", "https://www.googleapis.com/auth/drive.readonly", "https://www.googleapis.com/auth/script.container.ui"],
  "runtimeVersion": "V8"
}
